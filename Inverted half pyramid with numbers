import java.util.*;

class Main{
    public static void main(String args[])
    {
        Scanner obj=new Scanner(System.in);
        int lines=obj.nextInt();
        int num=lines;
        for(int i=0;i<lines;i++)
        {
            for(int j=1;j<=num-i;j++)
            {
                System.out.print(j);
            }
            System.out.println(" ");
        }
    }
}
